pages = {160--164},
publisher = {Blackwell Science Inc},
title = {{Mitochondrial DNA phylogeny of the symbiotic dinoflagellates (Symbiodinium, Dinophyta)}},
url = {papers3://publication/doi/10.1046/j.1529-8817.2004.03097.x},
volume = {40},
