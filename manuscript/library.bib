month = {feb},
number = {6},
pages = {2527--2531},
title = {{Flow enhances photosynthesis in marine benthic autotrophs by increasing the efflux of oxygen from the organism to the water.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=20133799{\&}retmode=ref{\&}cmd=prlinks papers3://publication/doi/10.1073/pnas.0912348107},
