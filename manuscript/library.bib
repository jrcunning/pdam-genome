journal = {J. Exp. Biol.},
month = {jan},
pages = {1192--1196},
publisher = {Co Biol},
title = {{Tenacious D: Symbiodinium in clade D remain in reef corals at both high and low temperature extremes despite impairment.}},
url = {http://jeb.biologists.org/lookup/doi/10.1242/jeb.148239 papers3://publication/doi/10.1242/jeb.148239},
