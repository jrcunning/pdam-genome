journal = {Biotechnol. J.},
month = {sep},
number = {9},
pages = {1035--1042},
title = {{Flux-coupled genes and their use in metabolic flux analysis.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=23420780{\&}retmode=ref{\&}cmd=prlinks papers3://publication/doi/10.1002/biot.201200279},
