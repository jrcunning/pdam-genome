journal = {FEMS Microbiol. Rev.},
month = {jul},
number = {4},
pages = {557--578},
publisher = {Blackwell Publishing Ltd},
title = {{Species divergence and the measurement of microbial diversity.}},
url = {http://doi.wiley.com/10.1111/j.1574-6976.2008.00111.x papers3://publication/doi/10.1111/j.1574-6976.2008.00111.x},
volume = {32},
year = {2008}
}
