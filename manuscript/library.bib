journal = {Environ. Toxicol. Chem.},
month = {dec},
number = {12},
pages = {3230--3237},
title = {{Physiological modes of action of toxic chemicals in the nematode Acrobeloides nanus.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=17220093{\&}retmode=ref{\&}cmd=prlinks papers3://publication/uuid/BD8D15A4-4F7A-4785-90AB-C284845258E1},
