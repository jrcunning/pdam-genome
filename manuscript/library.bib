journal = {Proc. R. Soc. B},
month = {dec},
number = {1797},
pages = {20141856},
publisher = {The Royal Society},
title = {{The reef-building coral Siderastrea siderea exhibits parabolic responses to ocean acidification and warming}},
url = {http://rspb.royalsocietypublishing.org.eres.library.manoa.hawaii.edu/content/281/1797/20141856.full papers3://publication/doi/10.1098/rspb.2014.1856},
