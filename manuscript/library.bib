pages = {934--945},
publisher = {Co Biol},
title = {{What are the physiological and immunological responses of coral to climate warming and disease?}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=20190118{\&}retmode=ref{\&}cmd=prlinks papers3://publication/doi/10.1242/jeb.037580},
