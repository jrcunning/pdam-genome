journal = {Mar. Biol.},
month = {jan},
number = {1},
pages = {19--27},
title = {{Fungi in corals: black bands and density-banding of Porites lutea and P-lobata skeleton}},
url = {papers3://publication/uuid/8A41B22B-24EB-4697-A0A2-F9578AD3C198},
volume = {136},
year = {2000}
}
