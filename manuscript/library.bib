journal = {Proc. Natl. Acad. Sci. U. S. A.},
month = {jan},
number = {1},
pages = {E139--E148},
title = {{Microbial biogeography of wine grapes is conditioned by cultivar, vintage, and climate}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1317377110 papers3://publication/doi/10.1073/pnas.1317377110},
volume = {111},
year = {2014}
