title = {{Getting the Hologenome Concept Right: an Eco-Evolutionary Framework for Hosts and Their Microbiomes}},
url = {http://msystems.asm.org/lookup/doi/10.1128/mSystems.00028-16 papers3://publication/doi/10.1128/mSystems.00028-16},
