title = {{From empirical patterns to theory: a formal metabolic theory of life}},
url = {http://rstb.royalsocietypublishing.org/cgi/doi/10.1098/rstb.2007.2230 papers3://publication/doi/10.1098/rstb.2007.2230},
volume = {363},
year = {2008}
}
