journal = {Environ. Entomol.},
month = {dec},
number = {6},
pages = {1384--1396},
publisher = {The Oxford University Press},
title = {{Dietary Benefits of Fungal Associates to an Eruptive Herbivore: Potential Implications of Multiple Associates on Host Population Dynamics}},
url = {http://ee.oxfordjournals.org/cgi/doi/10.1093/ee/36.6.1384 papers3://publication/doi/10.1093/ee/36.6.1384},
volume = {36},
year = {2007}
}
