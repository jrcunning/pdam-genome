journal = {Mar. Pollut. Bull.},
month = {feb},
number = {3-4},
pages = {327--335},
publisher = {Elsevier Ltd},
title = {{Comparing bleaching and mortality responses of hard corals between southern Kenya and the Great Barrier Reef, Australia.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=14972585{\&}retmode=ref{\&}cmd=prlinks papers3://publication/doi/10.1016/j.marpolbul.2003.08.024},
