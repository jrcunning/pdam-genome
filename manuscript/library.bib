doi = {10.3354/meps07588},
file = {:Users/jrcunning/Documents/Mendeley Desktop/D'Angelo et al/2008/D'Angelo et al.{\_}2008{\_}Marine Ecology Progress Series.pdf:pdf},
isbn = {0171-8630},
issn = {01718630},
journal = {Mar. Ecol. Prog. Ser.},
keywords = {Coral,Coral health,Fluorescence,Fluorescent protein,GFP,Green fluorescent protein,Light sensing,Light-induced protein expression,RFP,Red fluorescent protein},
