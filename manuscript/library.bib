journal = {Appl. Environ. Microbiol.},
month = {jan},
number = {4},
pages = {1379--1384},
publisher = {American Society for Microbiology (ASM)},
title = {{Effect of temperature on adhesion of Vibrio strain AK-1 to Oculina patagonica and on coral bleaching}},
