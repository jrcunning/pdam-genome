file = {:Users/jrcunning/Documents/Mendeley Desktop/Riegl, BRANCH/1995/Riegl, BRANCH{\_}1995{\_}Journal of Experimental Marine Biology and Ecology.pdf:pdf},
journal = {J. Exp. Mar. Bio. Ecol.},
month = {jan},
number = {2},
pages = {259--275},
publisher = {Elsevier B.V.},
title = {{Effects of sediment on the energy budgets of four scleractinian (Bourne 1900) and five alcyonacean (Lamouroux 1816) corals}},
url = {http://apps.isiknowledge.com/InboundService.do?product=WOS{\&}action=retrieve{\&}SrcApp=Papers{\&}UT=A1995QT95300006{\&}SID=4Dhkg2J2PI8dB85m{\%}40nl{\&}SrcAuth=mekentosj{\&}mode=FullRecord{\&}customersID=mekentosj{\&}DestFail=http{\%}3A{\%}2F{\%}2Faccess.isiproducts.com{\%}2Fcustom{\_}images{\%}2Fwo},
