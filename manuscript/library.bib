journal = {Bioinformatics},
month = {dec},
number = {24},
pages = {3435--3436},
publisher = {Oxford University Press},
title = {{PoPoolation2: identifying differentiation between populations using sequencing of pooled DNA samples (Pool-Seq).}},
url = {http://bioinformatics.oxfordjournals.org/content/27/24/3435.full papers3://publication/doi/10.1093/bioinformatics/btr589},
