file = {:Users/jrcunning/Documents/Mendeley Desktop/Jones, Kerswell/2003/Jones, Kerswell{\_}2003{\_}Marine Ecology Progress Series.pdf:pdf},
journal = {Mar. Ecol. Prog. Ser.},
month = {oct},
pages = {149--159},
publisher = {Inter-Research},
title = {{Phytotoxicity of Photosystem II (PSII) herbicides to coral}},
url = {http://apps.isiknowledge.com/InboundService.do?product=WOS{\&}action=retrieve{\&}SrcApp=Papers{\&}UT=000186704400012{\&}SID=4AlO1dgmA7HpkeEiIIf{\&}SrcAuth=mekentosj{\&}mode=FullRecord{\&}customersID=mekentosj{\&}DestFail=http{\%}3A{\%}2F{\%}2Faccess.isiproducts.com{\%}2Fcustom{\_}images{\%}2Fwok{\_}},
