journal = {Mar. Ecol. Prog. Ser.},
month = {jan},
pages = {75--89},
publisher = {Inter-Research},
title = {{Landscape-scale variation in coral recruitment in Moorea, French Polynesia}},
url = {http://www.int-res.com/abstracts/meps/v414/p75-89/ papers3://publication/doi/10.3354/meps08728},
