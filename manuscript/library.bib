title = {{Reserve design for uncertain responses of coral reefs to climate change}},
url = {http://links.isiglobalnet2.com/gateway/Gateway.cgi?GWVersion=2{\&}SrcAuth=mekentosj{\&}SrcApp=Papers{\&}DestLinkType=FullRecord{\&}DestApp=WOS{\&}KeyUT=000286599600007 papers3://publication/doi/10.1111/j.1461-0248.2010.01562.x},
