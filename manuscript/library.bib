journal = {PeerJ},
pages = {e2692},
pmid = {27896028},
title = {{The scavenger receptor repertoire in six cnidarian species and its putative role in cnidarian-dinoflagellate symbiosis}},
url = {https://peerj.com/articles/2692},
volume = {4},
year = {2016}
}
