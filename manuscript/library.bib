journal = {J. Exp. Biol.},
month = {may},
number = {Pt 9},
pages = {1533--1545},
publisher = {Co Biol},
title = {{Physiological responses of the scleractinian coral Pocillopora damicornis to bacterial stress from Vibrio coralliilyticus.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=21490261{\&}retmode=ref{\&}cmd=prlinks papers3://publication/doi/10.1242/jeb.053165},
