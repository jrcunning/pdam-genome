issn = {09621083},
journal = {Mol. Ecol.},
keywords = {FST outlier,Gasterosteus aculeatus,RAD,gene flow,next generation sequencing,population genomics,speciation},
number = {12},
pages = {2852--2862},
pmid = {22384978},
title = {{Genome divergence during evolutionary diversification as revealed in replicate lake-stream stickleback population pairs}},
volume = {21},
year = {2012}
}
