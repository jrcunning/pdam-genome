issn = {00129658},
journal = {Ecology},
keywords = {Biological invasion,Douglas-fir,Ectomycorrhizal fungi,Mutualisms,Plant-soil interactions,Pseudotsuga menziesii},
number = {9},
pages = {2336--2347},
pmid = {26594692},
title = {{Mycorrhizal co-invasion and novel interactions depend on neighborhood context}},
volume = {96},
year = {2015}
}
