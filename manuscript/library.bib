isbn = {1365-2486},
issn = {13541013},
journal = {Glob. Chang. Biol.},
keywords = {18 august 2014,august 2014 and accepted,coral reefs,degraded ecosystems,ecosystem function,ecosystem services,habitat complexity,marine reserve,received 6 june 2014,revised version received 18},
number = {2},
pages = {504--514},
pmid = {25179273},
title = {{Anticipative management for coral reef ecosystem services in the 21st century}},
