journal = {J. Exp. Biol.},
month = {dec},
number = {Pt 24},
pages = {4304--4312},
publisher = {Co Biol},
title = {{Shallow-water wave lensing in coral reefs: a physical and biological case study.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=21113012{\&}retmode=ref{\&}cmd=prlinks papers3://publication/doi/10.1242/jeb.044941},
