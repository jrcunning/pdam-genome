journal = {Mar. Pollut. Bull.},
month = {may},
number = {5},
pages = {745--751},
publisher = {Elsevier Ltd},
title = {{Water quality and coral bleaching thresholds: formalising the linkage for the inshore reefs of the Great Barrier Reef, Australia.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=19230930{\&}retmode=ref{\&}cmd=prlinks papers3://publication/doi/10.1016/j.marpolbul.2008.12.013},
