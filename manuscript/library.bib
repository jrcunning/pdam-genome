journal = {Mar. Pollut. Bull.},
month = {jan},
pages = {495--506},
publisher = {Elsevier Ltd},
title = {{The ecotoxicological effects of Photosystem II herbicides on corals}},
url = {papers3://publication/uuid/FCBB9636-BF32-4A36-BCB0-565719EC6E64},
