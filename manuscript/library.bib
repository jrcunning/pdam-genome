journal = {Protein Sci.},
keywords = {domains,structure prediction,vertebrate fibrinogen},
number = {12},
pages = {1563--1577},
pmid = {1304888},
title = {{A detailed consideration of a principal domain of vertebrate fibrinogen and its relatives}},
volume = {1},
year = {1992}
}
