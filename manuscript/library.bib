journal = {Proc. R. Soc. B},
month = {nov},
number = {1842},
pages = {20161918--20161938},
publisher = {The Royal Society},
title = {{Symbiodinium biogeography tracks environmental patterns rather than host genetics in a key Caribbean reef-builder, Orbicella annularis}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0960982213004284 papers3://publication/doi/10.1098/rspb.2016.1938},
