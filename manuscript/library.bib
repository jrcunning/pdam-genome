pages = {e76432},
publisher = {Public Library of Science},
title = {{Is Physiological Performance a Good Predictor for Fitness? Insights from an Invasive Plant Species}},
url = {http://dx.plos.org/10.1371/journal.pone.0076432 papers3://publication/doi/10.1371/journal.pone.0076432},
