journal = {Environ. Microbiol.},
month = {jan},
number = {1},
pages = {268--276},
title = {{Apoptosis as a post-phagocytic winnowing mechanism in a coral-dinoflagellate mutualism.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=19125818{\&}retmode=ref{\&}cmd=prlinks papers3://publication/doi/10.1111/j.1462-2920.2008.01774.x},
