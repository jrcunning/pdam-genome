journal = {PLoS One},
month = {jan},
number = {5},
pages = {e20392},
publisher = {Public Library of Science},
title = {{Rapid evolution of coral proteins responsible for interaction with the environment.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=21633702{\&}retmode=ref{\&}cmd=prlinks papers3://publication/doi/10.1371/journal.pone.0020392},
