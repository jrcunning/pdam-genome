journal = {PNAS},
month = {feb},
number = {5},
pages = {1525--1530},
publisher = {National Acad Sciences},
title = {{Stable isotopes (delta13C and delta15N) of organic matrix from coral skeleton.}},
url = {http://www.pnas.org/content/102/5/1525.full papers3://publication/doi/10.1073/pnas.0408921102},
volume = {102},
year = {2005}
}
