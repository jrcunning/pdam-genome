journal = {Mol. Cell. Probes},
month = {dec},
number = {6},
pages = {339--348},
title = {{Development of a fungus-specific PCR assay for detecting low-level fungi in an indoor environment.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=11090263{\&}retmode=ref{\&}cmd=prlinks papers3://publication/doi/10.1006/mcpr.2000.0324},
volume = {14},
year = {2000}
}
