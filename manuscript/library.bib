file = {:Users/jrcunning/Documents/Mendeley Desktop/Samuk et al/2017/Samuk et al.{\_}2017{\_}Molecular ecology.pdf:pdf},
isbn = {4955139574},
issn = {1365-294X},
journal = {Mol. Ecol.},
keywords = {adaptation,empirical,gene flow,genomics,hybridization,population genetics},
number = {June},
pages = {4378--4390},
pmid = {28667780},
primaryClass = {arXiv:physics},
title = {{Gene flow and selection interact to promote adaptive divergence in regions of low recombination.}},
year = {2017}
