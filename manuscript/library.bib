file = {:Users/jrcunning/Documents/Mendeley Desktop/Ramos-Silva et al/2013/Ramos-Silva et al.{\_}2013{\_}Molecular Biology and Evolution.pdf:pdf},
isbn = {1537-1719 (Electronic)$\backslash$r0737-4038 (Linking)},
issn = {07374038},
journal = {Mol. Biol. Evol.},
keywords = {biomineralization,calcium carbonate skeleton,evolution,proteomics,scleractinian},
number = {9},
pages = {2099--2112},
pmid = {23765379},
title = {{The skeletal proteome of the coral acropora millepora: The evolution of calcification by co-option and domain shuffling}},
volume = {30},
year = {2013}
}
