journal = {J. Exp. Biol.},
month = {feb},
number = {Pt 4},
pages = {651--658},
publisher = {Company of Biologists},
title = {{Redox signaling in the growth and development of colonial hydroids.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=12517982{\&}retmode=ref{\&}cmd=prlinks papers3://publication/uuid/2DD9554E-B749-4CC7-8B6D-A4243886C39C},
volume = {206},
