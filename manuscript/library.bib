url = {http://apps.isiknowledge.com/InboundService.do?product=WOS{\&}action=retrieve{\&}SrcApp=Papers{\&}UT=000251102300004{\&}SID=4FeMFE29PnF6E8B8liG{\&}SrcAuth=mekentosj{\&}mode=FullRecord{\&}customersID=mekentosj{\&}DestFail=http{\%}3A{\%}2F{\%}2Faccess.isiproducts.com{\%}2Fcustom{\_}images{\%}2Fwok{\_}},
