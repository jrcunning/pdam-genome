author = {Okamoto, M and Nojima, Satoshi and Furushima, Y and Nojima, H},
file = {:Users/jrcunning/Documents/Mendeley Desktop/Okamoto et al/2005/Okamoto et al.{\_}2005{\_}Fisheries Science.pdf:pdf},
journal = {Fish. Sci.},
month = {jan},
number = {4},
pages = {847--854},
title = {{Evaluation of coral bleaching condition in situ using an underwater pulse amplitude modulated fluorometer}},
url = {papers3://publication/uuid/2F4149F5-1F31-45A1-852F-741510ED8F5C},
volume = {71},
year = {2005}
}
