publisher = {Elsevier Ltd},
title = {{Reef corals bleach to resist stress}},
url = {http://www.sciencedirect.com/science?{\_}ob=ArticleURL{\&}{\_}udi=B6V6N-4TW53GF-4{\&}{\_}user=687815{\&}{\_}rdoc=1{\&}{\_}fmt={\&}{\_}orig=search{\&}{\_}sort=d{\&}view=c{\&}{\_}acct=C000038378{\&}{\_}version=1{\&}{\_}urlVersion=0{\&}{\_}userid=687815{\&}md5=862729781aa4cc1c78db742d0c970d80 papers3://publication/doi/10.1016},
