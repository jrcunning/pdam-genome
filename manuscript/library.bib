pages = {163--181},
publisher = {Elsevier B.V.},
title = {{Photo-acclimation of the hermatypic coral Stylophora pistillata while subjected to either starvation or food provisioning}},
url = {papers3://publication/uuid/E8451D44-5DEC-4353-9B2E-8F83B6E1334D},
